<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mbase.sample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activitys.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.NetworkActivity"
            android:label="网络测试"
            android:screenOrientation="portrait" />
        <activity android:name=".activitys.ImageActivity"
            android:label="加载图片"
            android:screenOrientation="portrait" />
        <activity android:name=".activitys.SqliteActivity"
            android:label="本地数据库测试"
            android:screenOrientation="portrait" />
        <activity android:name=".activitys.OtherActivity"
            android:label="其它测试"
            android:screenOrientation="portrait" />
    </application>

</manifest>
